print(f"si{mylist}")'''
#插p入数据
'''mylist=["ither","jided","fsfeu"]
index=mylist.index("ither")
mylist.insert(index,1)
print(mylist)'''
#追加元素1
'''mylist=["ither","jided","fsfeu"]
mylist.append(4)
print(mylist)'''
#追加元素2
'''mylist=["ither","jided","fsfeu"]
mylist.append([2,3,4])
print(mylist)'''
#追加元素3
'''mylist=["ither","jided","fsfeu"]
mylist.extend([2,3,4])
print(mylist)'''
#删除数据
'''mylist=["ither","jided","fsfeu"]
mylist.pop(2)
print(mylist)'''
#删除数据2
'''mylist=["ither","jided","fsfeu"]
del mylist[2]
print(mylist)'''
#通过remove方法移除重复数据的第一个值
'''mylist=["ither","ither","jided","fsfeu"]
mylist.remove("ither")
print(mylist)'''
#清空列表
'''mylist=["ither","ither","jided","fsfeu"]
mylist.clear()
print(mylist)'''
#统计某元素在列表中的数量
'''mylist=["ither","ither","jided","fsfeu"]
print(mylist.count("ither"))'''
#列表共有多少个元素
'''mylist=["ither","ither","jided","fsfeu"]
print(len(mylist))'''
