#count查元素,看长度
'''t5=('i','h','i')
count=t5.count('i')
num=len(t5)
print(count)
print(num)'''
#元组的遍历while
'''t5=('i','h','m',1,2,3,3)
index=0
while index<len(t5):
    print(t5[index])
    index+=1'''
#元组的遍历for
'''t5=('i','h','m',1,2,3,3)
for x in t5:
    print(x)
'''
#字符串
'''my_str='ithskrb sduibdf'
